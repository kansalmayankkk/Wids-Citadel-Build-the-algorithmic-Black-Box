   "source": [
    "import numpy as np\n",
    "\n",
    "def sharpe(returns):\n",
    "    return np.mean(returns) / (np.std(returns) + 1e-8)\n",
    "\n",
    "def max_drawdown(values):\n",
    "    peak = values[0]\n",
    "    max_dd = 0\n",
    "    for v in values:\n",
    "        peak = max(peak, v)\n",
    "        max_dd = max(max_dd, (peak - v) / peak)\n",
    "    return max_dd\n"
   ]
